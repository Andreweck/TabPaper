    #div1 {
        height: 20px;
        width: 20px;
        margin: 3px;
        padding: 3px;
        border: 1px dotted #333333;
    }

    #div2 {
    }

    #newId {
    }

    #BinDiv {
        height: 40px;
        width: 20px;
        border: 3px double #0026ff;
    }
</style>
<script src="~/Scripts/DrgnDrp.js">

</script>

<script>


    var cars = [];
    var text = [];
    var i;
    function frlp() {
    for (i = 0; i < cars.length; i++) {
        text += [(cars[i] + "<br>")];

    }
        document.getElementById("zzzz").innerHTML = text;
    }
    function append(a, b, c, img) {

        cars.push(a + b + c + img);
        frlp();
    }
    function remove() {
        cars.splice(cars[0], 1);
        document.getElementById("zzzz").innerHTML = cars;
    }
</script>
@*change above and below*@

<h2>@ViewBag.Title</h2>

@*; border: 1px dotted #333333; height:40px; width:auto*@


@for (int s = 0; s < 8; s++)
{<!--Individual Line-->
    <div id="div2">
        @for (int i = 0; i < 7; i++)
        {

            <div style="clear:left; background-image:url('/Content/Images/TbPpr.png')">
                <!--Tab strings-->
                <p style="float:left"><b>@(s + 1) :</b></p><p style="float:left">@ViewBag.range[i]</p><p style="float:left">|</p>
                @for (int b = 0; b < 23; b++)
                {
                    <!--Tab timeline-->
                    <p style="float:left">&#8213</p>

                    <div style="float:left" id="div1" ondrop="clickCounter(); copy(event, @i)" ondragover="allowDrop(event)">
                    </div>


                    <p style="float:left">|</p>


                }
            </div>


        }
        <div style="clear:left" ondragover="allowDrop(event)" ondrop="delete (event)">
            <!--Note Values:-->
            @for (int t = 0; t < 33; t++)
            {

                <img id="@("drag" + t.ToString())" src="/Content/Images/Int_Square_Blank(@t).png" style="float:left" height="20px" width="20px" draggable="true" ondragstart="drag(event)" dropzone="BinDiv"><b style="float:left"> | </b>

            }
        </div>
        <div id="BinDiv" ondragover="allowDrop(event)" ondrop="drop(event)"></div>

    </div>
}

